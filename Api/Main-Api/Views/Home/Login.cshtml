@model Domain.LoginRequest
@{
    ViewData["Title"] = "Login";
}


<div class="text-center">
    <h1 class="display-4">Feel Free To Login SUSY</h1>
    @using (Html.BeginForm("Login", "Home", FormMethod.Post))
    {
        <div>
            @Html.LabelFor(m => m.Email)
            @Html.TextBoxFor(expression: m => m.Email, new { @class = "form-control" })
        </div>
        <div>
            @Html.LabelFor(m => m.Password)
            @Html.PasswordFor(expression: m => m.Password, new { @class = "form-control" })
        </div>
        <div class="m-2">
            <button type="submit" class="btn btn-primary">Login</button>
        </div>
    }
</div>